<script>
    var metrics=window.DVWebNode.perfMetrics['pangaea-banner'];
    var i=metrics.length-1;
    metrics[i].afterProps=new Date();
    if(window.ue!=undefined && typeof window.ue.count==='function'){
        window.ue.count('DVWebNode:pangaea-banner:'+i+':DOMParsed', metrics[i].afterMarkup.getTime() - metrics[i].beforeMarkup.getTime());
        window.ue.count('DVWebNode:pangaea-banner:'+i+':PropsParsed',metrics[i].afterProps.getTime() - metrics[i].afterMarkup.getTime());
    }
</script>
    

<script>
    DVWebNode=window.DVWebNode||{};
    DVWebNode.perfMetrics=DVWebNode.perfMetrics||{};
    DVWebNode.perfMetrics['storefront']=DVWebNode.perfMetrics['storefront']||[];
    DVWebNode.perfMetrics['storefront'].push({beforeMarkup:new Date()});
</script>
